{
    "name": "arsinex/currency",
    "description": "digital maney , laravel , currency",
    "keywords": ["digital maney , laravel , currency"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mahmood Bagheri",
            "email": "mail.mahmood.bagheri@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0"
    },
    "autoload": {
        "psr-4": {
            "Arsinex\\Currency\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arsinex\\Currency\\CurrencyServiceProvider"
            ]
        }
    },

    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
